<section class="dashboard-body">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="panel">
                        <div class="dashboard-logo-sidebar"> <img class="img-responsive center-block" src="/asset/images/company/logo1.jpg" alt="Image"> </div>
                        <div class="text-center dashboard-logo-sidebar-title">
                            <h4>Your Company Agency Pvt. Ltd</h4>
                        </div>
                    </div>

                    {{> comnav}}

                </div>
                <div class="col-md-8 col-sm-8 col-xs-12">
                    <div class="job-short-detail">
                        <div class="heading-inner">
                            <p class="title">Profile detail</p>
                        </div>
                        <dl>
                            <dt>Student Id:</dt>
                            <dd>{{prof.roll}}</dd>
                            <dt>CNIC:</dt>
                            <dd>{{prof.cnic}}</dd>
                            <dt>Date Of Birth:</dt>
                            <dd> {{prof.DOB}} </dd>
                            <dt>Phone:</dt>
                            <dd>{{prof.Phone}}</dd>
                            <dt>Email:</dt>
                            <dd>{{prof.email}}</dd>
                            <dt>Address:</dt>
                            <dd>{{prof.address}}</dd>
                            <dt>Country:</dt>
                            <dd>Pakistan</dd>
                        </dl>
                    </div>
                    <div class="resume-box">
                        <div class="heading-inner">
                            <p class="title">Objective</p>
                        </div>
                        <div class="col-md-12 col-xs-12 col-sm-12">
                            {{{doc.objective}}}
                        </div>
                    </div>
                    <div class="resume-box">
                        <div class="heading-inner">
                            <p class="title">Educational Information</p>
                        </div>
                        {{#each doc.education}}
                        <div class="row education-box">
                            <div class="col-md-4 col-xs-12 col-sm-4">
                                <div class="resume-icon">
                                    <span class="icon-pencil"></span>
                                </div>
                                <div class="insti-name">
                                    <h4>{{this.inst}}</h4>
                                    <span>{{this.session}}</span>
                                </div>
                            </div>
                            <div class="col-xs-12 col-md-8 col-sm-8">
                                <div class="degree-info">
                                    <h4>{{this.degree}}</h4>
                                    <p>{{this.marks}}</p>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="resume-box">
                            <div class="heading-inner">
                                <p class="title">Work Experience</p>
                            </div>
                            {{#each doc.experience}}
                            <div class="row education-box">
                                <div class="col-md-6 col-xs-12 col-sm-6">
                                    <div class="resume-icon">
                                        <span class="icon-clipboard"></span>
                                    </div>
                                    <div class="insti-name">
                                        <h4>{{this.company}}</h4> <br>

                                        <span>{{this.startDate}} to {{this.endDate}}</span>
                                    </div>
                                </div>
                               <div class="col-md-6 col-xs-8 col-sm-6">
                                   <h4>{{this.designation}}</h4>
                               </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>


                </div>

            </div>
            <div class="col-md-12 col-sm-12 col-xs-12">

                <div class="col-md-6 col-sm-6 col-xs-6">
                    <div class="heading-inner">
                        <p class="title">Skills</p>
                    </div>
                    {{#each doc.skills}}
                    <div class="progress">
                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="5" aria-valuemin="0" aria-valuemax="10" style="width: 90%">
                            <span class="sr-only">{{this.rate}}% Complete (success)</span>
                        </div>
                        <span class="progress-type">{{this.name}}</span>
                        <span class="progress-completed">{{this.rate}}%</span>
                    </div>
                    {{/each}}
                </div>
                <div class="col-md-6 col-sm-6 col-xs-6">
                    <div class="heading-inner">
                        <p class="title">Achivements</p>
                    </div>
                    <div class="col-md-12 col-xs-12 col-sm-12">
                        <h4>{{doc.achievements}}</h4>
                    </div>
                </div>

            </div>
            <div class="heading-inner">
                <p class="title">Area of Interest</p>
            </div>
            <div class="col-md-12 col-xs-12 col-sm-12">
                <h4>{{doc.areaOfInterests}}</h4>
            </div>
        </div>
        </div>
    </div>
</section>